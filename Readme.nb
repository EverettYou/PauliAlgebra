Cell["Four basic Pauli matrices are introduced", "TextIndent"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["\[Sigma]", "0"], "=", 
    RowBox[{"(", GridBox[{
       {"1", "0"},
       {"0", "1"}
      }], ")"}]}], ",", 
   RowBox[{
    SubscriptBox["\[Sigma]", "1"], "=", 
    RowBox[{"(", GridBox[{
       {"0", "1"},
       {"1", "0"}
      }], ")"}]}], ",", 
   RowBox[{
    SubscriptBox["\[Sigma]", "2"], "=", 
    RowBox[{"(", GridBox[{
       {"0", 
        RowBox[{"-", "\[ImaginaryI]"}]},
       {"\[ImaginaryI]", "0"}
      }], ")"}]}], ",", 
   RowBox[{
    SubscriptBox["\[Sigma]", "3"], "=", 
    RowBox[{"(", GridBox[{
       {"1", "0"},
       {"0", 
        RowBox[{"-", "1"}]}
      }], ")"}]}], ","}], TraditionalForm]], "DisplayFormulaNumbered"],

Cell[TextData[{
 "Denote the tensor product of a series of Pauli matrices ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Sigma]", "a"], "\[CircleTimes]", 
    SubscriptBox["\[Sigma]", "b"], "\[CircleTimes]", 
    SubscriptBox["\[Sigma]", "c"], "\[CircleTimes]", "\[Ellipsis]"}], 
   TraditionalForm]]],
 " by the short-handed notation ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Sigma]", 
    RowBox[{"a", ",", "b", ",", "c", ",", "\[Ellipsis]"}]], TraditionalForm]]],
 ", which can be input either as ",
 StyleBox["\[Sigma][a,b,c,\[Ellipsis]]", "Program"],
 " directly, or can explicitly use the tensor product operator \
(\[CircleTimes] is entered as \[EscapeKey]c*\[EscapeKey]). The tensor product \
construction will be translated by ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " to the short-handed notation automatically."
}], "TextNoIndent"],

