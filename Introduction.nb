Cell[BoxData[
 RowBox[{"Det", "[", 
  RowBox[{
   RowBox[{"b", " ", 
    RowBox[{"\[Sigma]", "[", 
     RowBox[{"1", ",", "3"}], "]"}]}], "+", 
   RowBox[{"c", " ", 
    RowBox[{"\[Sigma]", "[", 
     RowBox[{"2", ",", "2"}], "]"}]}]}], "]"}]], "Input"],

Cell[BoxData[
 RowBox[{
  SuperscriptBox["b", "4"], "-", 
  RowBox[{"2", " ", 
   SuperscriptBox["b", "2"], " ", 
   SuperscriptBox["c", "2"]}], "+", 
  SuperscriptBox["c", "4"]}]], "Output"]
}, Open  ]],

